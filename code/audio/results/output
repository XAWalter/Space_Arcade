#! /usr/bin/vvp
:ivl_version "11.0 (stable)" "(v11_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x564a5c082090 .scope module, "audio_tb" "audio_tb" 2 2;
 .timescale -9 -12;
v0x564a5c09c860_0 .var "clk", 0 0;
v0x564a5c09c900_0 .net "pulse", 0 0, L_0x564a5c09d380;  1 drivers
S_0x564a5c0823b0 .scope module, "an01" "audio_note" 2 9, 3 1 0, S_0x564a5c082090;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "i_clk";
    .port_info 1 /OUTPUT 1 "o_pulse";
P_0x564a5c0722d0 .param/l "HIGH" 0 3 12, C4<1>;
P_0x564a5c072310 .param/l "LOW" 0 3 11, C4<0>;
L_0x564a5c09d380 .functor BUFZ 1, v0x564a5c09c6f0_0, C4<0>, C4<0>, C4<0>;
v0x564a5c048cd0_0 .var "c_state", 0 0;
v0x564a5c09c4b0_0 .var/i "i", 31 0;
v0x564a5c09c590_0 .net "i_clk", 0 0, v0x564a5c09c860_0;  1 drivers
v0x564a5c09c630_0 .net "o_pulse", 0 0, L_0x564a5c09d380;  alias, 1 drivers
v0x564a5c09c6f0_0 .var "r_pulse", 0 0;
E_0x564a5c06be60 .event posedge, v0x564a5c09c590_0;
S_0x564a5c082220 .scope module, "top" "top" 4 1;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "CLK";
    .port_info 1 /OUTPUT 1 "P1A1";
o0x7f95f875b1c8 .functor BUFZ 1, C4<z>; HiZ drive
