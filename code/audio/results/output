T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564a5c09c860_0, 0, 1;
    %delay 50000, 0;
T_2.0 ;
    %load/vec4 v0x564a5c09c860_0;
    %inv;
    %store/vec4 v0x564a5c09c860_0, 0, 1;
    %delay 50000, 0;
    %jmp T_2.0;
    %end;
