T_3 ;
    %vpi_call 2 23 "$dumpfile", "results/simx.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x564a5c082090 {0 0 0};
