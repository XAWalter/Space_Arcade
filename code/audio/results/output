    .port_info 1 /OUTPUT 1 "o_pulse";
P_0x564a5c09cb80 .param/l "HIGH" 0 3 12, C4<1>;
P_0x564a5c09cbc0 .param/l "LOW" 0 3 11, C4<0>;
L_0x564a5c09d440 .functor BUFZ 1, v0x564a5c09d080_0, C4<0>, C4<0>, C4<0>;
v0x564a5c09cd30_0 .var "c_state", 0 0;
v0x564a5c09ce10_0 .var/i "i", 31 0;
v0x564a5c09cef0_0 .net "i_clk", 0 0, o0x7f95f875b1c8;  alias, 0 drivers
v0x564a5c09cfc0_0 .net "o_pulse", 0 0, L_0x564a5c09d440;  alias, 1 drivers
v0x564a5c09d080_0 .var "r_pulse", 0 0;
E_0x564a5c073fc0 .event posedge, v0x564a5c09cef0_0;
    .scope S_0x564a5c0823b0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564a5c048cd0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x564a5c09c4b0_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x564a5c0823b0;
T_1 ;
    %wait E_0x564a5c06be60;
    %load/vec4 v0x564a5c048cd0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %jmp T_1.2;
T_1.0 ;
    %load/vec4 v0x564a5c09c4b0_0;
    %cmpi/s 24000, 0, 32;
    %jmp/0xz  T_1.3, 5;
    %load/vec4 v0x564a5c09c4b0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x564a5c09c4b0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x564a5c048cd0_0, 0;
    %jmp T_1.4;
T_1.3 ;
    %vpi_call 3 33 "$display", "%b", v0x564a5c048cd0_0 {0 0 0};
